{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "ordered-imports": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "no-empty": [
      true,
      "allow-empty-functions"
    ]
  },
  "rulesDirectory": []
}